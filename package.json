{
  "name": "owasp-docker-security",
  "version": "0.0.0",
  "description": "OWASP Docker Security",
  "dependencies": {},
  "devDependencies": {
    "gitbook-cli": "^2.3.2"
  },
  "scripts": {
    "serve": "node_modules/.bin/gitbook serve",
    "build:pdf": "node_modules/.bin/gitbook pdf ./ ./dist/owasp-docker-security.pdf",
    "build:epub": "node_modules/.bin/gitbook epub ./ ./dist/owasp-docker-security.epub",
    "build:mobi": "node_modules/.bin/gitbook mobi ./ ./dist/owasp-docker-security.mobi",
    "build": "npm run build:pdf; npm run build:epub; npm run build:mobi"
  }
}
